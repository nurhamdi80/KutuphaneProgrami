
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="../plugins/images/favicon.png">
    <title>403 Not Found</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Content/Css/bootstrap.min.css" rel="stylesheet" />
    <!-- animation CSS -->
    <link href="~/Content/Css/animate.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/Content/Css/style.css" rel="stylesheet" />
    <!-- color CSS -->
    <link href="~/Content/Css/blue.css" rel="stylesheet" />
   
</head>
<body>
    <!-- Preloader -->
    <div class="preloader">
        <div class="cssload-speeding-wheel"></div>
    </div>
    <section id="wrapper" class="error-page">
        <div class="error-box">
            <div class="error-body text-center">
                <h1>403</h1>
                <h3 class="text-uppercase">Forbidden Error</h3>
                <p class="text-muted m-t-30 m-b-30 text-uppercase">Bu sunucuya erişim izniniz yok!</p>
                <a href="@Url.Action("Index","Kitap")" class="btn btn-info btn-rounded waves-effect waves-light m-b-40">Giriş Sayfasına Geri Dön</a>
            </div>
            <footer class="footer text-center"> 2022 &copy; Kütüphane Programı - Nurettin HAMDİOĞLU </footer>
        </div>
    </section>
    <!-- jQuery -->
    <script src="~/Scripts/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Scripts/custom.js"></script>

</body>
</html>

